<Window x:Class="EkzamenUtkin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EkzamenUtkin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Height="Auto" HorizontalAlignment="left" VerticalAlignment="center" Orientation="Horizontal">
        </StackPanel>

        <Frame x:Name="frmMain" Content="Frame" Grid.RowSpan="3" />

        <StackPanel Grid.Row="2" VerticalAlignment="Center">

        </StackPanel>
    </Grid>
</Window>
